{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "../src",
        "eslint:recommended",
        "prettier"
    ],
    "overrides": [],
    "root": true,
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["error", "never"],
        "quotes": ["error", "double"],
        "indent": ["error", 4]
    }
}
